{
    "breadcrumbs.filePath": "off",
    "breadcrumbs.showNumbers": false,
    "debug.console.fontFamily": "Iosevka Nerd Font",
    "debug.console.fontSize": 15,
    "debug.console.lineHeight": 36,
    "debug.toolBarLocation": "docked",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": false,
    "editor.codeLens": false,
    "editor.colorDecorators": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "block",
    "editor.folding": false,
    "editor.foldingHighlight": false,
    "editor.fontFamily": "'Iosevka Nerd Font'",
    "editor.fontLigatures": true,
    "editor.fontSize": 20,
    "editor.formatOnSave": true,
    "editor.glyphMargin": false,
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineNumbers": "on",
    "editor.matchBrackets": "near",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.showFoldingControls": "never",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "jupyter.kernels.trusted": [
        "~/.local/python/bin"
    ],
    "markdown.preview.markEditorSelection": false,
    "notebook.cellFocusIndicator": "border",
    "notebook.cellToolbarLocation": {
        "default": "left"
    },
    "notebook.cellToolbarVisibility": "hover",
    "notebook.formatOnSave.enabled": true,
    "notebook.showCellStatusBar": "hidden",
    "notebook.showFoldingControls": "never",
    "outline.icons": false,
    "outline.showNumbers": false,
    "problems.decorations.enabled": false,
    "python.defaultInterpreterPath": "~/.local/python/bin",
    "scm.diffDecorations": "none",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 20,
    "window.autoDetectColorScheme": true,
    "window.autoDetectHighContrast": false,
    "window.closeWhenEmpty": true,
    "window.commandCenter": true,
    "window.customTitleBarVisibility": "auto",
    "window.enableMenuBarMnemonics": false,
    "window.newWindowDimensions": "inherit",
    "window.title": "${rootName}",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.location": "top",
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.languageDetection": false,
    "workbench.editor.languageDetectionHints": {
        "untitledEditors": false,
        "notebookEditors": false
    },
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.showIcons": false,
    "workbench.editor.showTabs": "single",
    "workbench.editor.tabSizing": "shrink",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.list.horizontalScrolling": true,
    "workbench.preferredDarkColorTheme": "Moegi Dark",
    "workbench.preferredLightColorTheme": "Adwaita Light",
    "workbench.productIconTheme": "adwaita",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "none",
    "explorer.fileNesting.enabled": true,
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    "git.openRepositoryInParentFolders": "always",
    "workbench.editor.editorActionsLocation": "titleBar",
    "codesnap.backgroundColor": "rgba(0, 0, 0, 0) ",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0)",
    "codesnap.containerPadding": "1em",
    "codesnap.showWindowControls": false,
    "codesnap.transparentBackground": true,
    "breadcrumbs.enabled": false,
    "terminal.external.linuxExec": "alacritty",
}